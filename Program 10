int main()
{
    int a[100],i,n,temp;
   
    printf("Enter size of the  array : ");
    scanf("%d", &n);
    printf("Enter elements in array : ");
    for(i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }
     printf("Enter the key : ");
    scanf("%d", &temp);
     
    for(i=0; i<n; i++)
    {
        if(a[i]==temp)
        {
			printf("element found ");
            return 0;		 
        }
 
    }
    
	printf("element  not  found");
}
